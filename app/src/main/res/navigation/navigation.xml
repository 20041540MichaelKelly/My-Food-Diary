<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/myFoodDiaryFragment">

    <fragment
        android:id="@+id/myFoodDiaryFragment"
        android:name="org.wit.myfooddiary.MyFoodDiaryFragment"
        android:label="fragment_my_food_diary"
        tools:layout="@layout/fragment_my_food_diary" >
        <action
            android:id="@+id/action_myFoodDiaryFragment_to_myFoodListFragment"
            app:destination="@id/myFoodListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/myFoodListFragment"
        android:name="org.wit.myfooddiary.MyFoodListFragment"
        android:label="fragment_my_food_list"
        tools:layout="@layout/fragment_my_food_list" />
</navigation>